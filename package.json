{
  "name": "jules-ui",
  "description": "The modern, developer-first interface for Google's Jules AI agent. Features live code diffs, interactive terminal outputs, analytics, and a streamlined dark mode experience.",
  "version": "0.8.8",
  "keywords": [
    "jules-ai",
    "google-jules",
    "ai-agent",
    "developer-tools",
    "nextjs",
    "shadcn-ui",
    "code-viewer",
    "git-diff",
    "dashboard"
  ],
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "server"
  ],
  "scripts": {
    "dev": "concurrently -n \"TERMINAL,NEXT\" -c \"blue,magenta\" \"npm:terminal:start\" \"next dev\"",
    "dev:e2e": "concurrently -n \"TERMINAL,NEXT\" -c \"blue,magenta\" \"npm:terminal:start\" \"next dev -p 3005\"",
    "terminal:start": "cd terminal-server && node server.js",
    "prebuild": "node scripts/get-submodule-info.js",
    "build": "pnpm run build:shared && npx prisma generate --schema=prisma/schema.prisma && next build",
    "build:shared": "pnpm --filter @jules/shared build",
    "build:cli": "pnpm --filter @jules/cli build",
    "build:all": "pnpm run build:shared && pnpm run build && pnpm run build:cli",
    "start": "next start",
    "daemon": "bun run server/index.ts",
    "lint": "eslint",
    "test": "jest",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit && pnpm --filter @jules/shared typecheck && pnpm --filter @jules/cli typecheck",
    "postinstall": "pnpm run build:shared && npx prisma generate --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hono/node-server": "^1.19.7",
    "@hookform/resolvers": "^3.1.0",
    "@jules/shared": "workspace:*",
    "@libsql/client": "^0.6.0",
    "@prisma/adapter-libsql": "^5.19.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/themes": "^3.2.1",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-virtual": "^3.13.18",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "bun-pty": "^0.4.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.25",
    "hono": "^4.11.3",
    "lucide-react": "^0.555.0",
    "motion": "^12.23.25",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "octokit": "^5.0.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.51.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^4.0.8",
    "recharts": "^3.5.1",
    "remark-gfm": "^4.0.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "sonner": "^2.0.7",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4",
    "tunnel-rat": "^0.1.2",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@prisma/client": "^5.19.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "jose": "^6.1.3",
    "prisma": "^5.19.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "optionalDependencies": {
    "lightningcss": "^1.30.2"
  }
}
